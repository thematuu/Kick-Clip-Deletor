{
    "manifest_version": 3,
    "name": "Kick Deletor",
    "version": "0.1",
    "description": "Delete clips and VODs from your Kick channel automatically",
    "permissions": ["storage","cookies"],
    "host_permissions": ["https://kick.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  